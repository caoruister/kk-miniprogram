.weui-form-pre {
  margin-bottom: 10px;
  background-color: #fff;
}

.weui-form-preview__bd {
  padding-right: 0;
  padding-top: 0;
  padding-bottom: 0;
}

.weui-form-preview__item {
  padding: 10rpx 0;
  border-top: 1rpx solid #d9d9d9;
}

.weui-form-preview__item:first-child {
  border-top: 0;
}

.weui-form-preview__label {
  font-size: 16px;
  color: #000;
}

.weui-form-preview__value {
  text-align: left;
  font-size: 16px;
}

.view-img {
  width: 150rpx;
  height: 150rpx;
  margin:20rpx;
}

.view-img image{
  width: 100%;
  height: 100%;
}